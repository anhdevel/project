{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:@angular-eslint/recommended",
        "plugin:@angular-eslint/template/process-inline-templates",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "@angular-eslint"
    ],
    "rules": {
        "@typescript-eslint/explicit-function-return-type": 2
    }
}